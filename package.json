{
  "name": "direktorium",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gerhardbraun.github.io/Direktorium/",
  "type": "module",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@tanstack/react-virtual": "^3.11.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.3.2",
    "ical-generator": "^8.1.1",
    "lucide-react": "^0.263.1",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-scripts": "5.0.1",
    "read-excel-file": "^6.0.1",
    "source-map-explorer": "^2.5.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "convert": "ts-node direktorium-converter.js",
    "convert:dev": "ts-node direktorium-converter.js dev",
    "convert:2026": "ts-node direktorium-converter.js 2026",
    "dli": "python C:/AppProjekte/excel-liturgical-converter/dli-converter.py",
    "messe": "python C:/AppProjekte/excel-liturgical-converter/ms-converter.py",
    "footnotes": "python C:/AppProjekte/excel-liturgical-converter/dli-footnotes.py",
    "catalogue": "python C:/AppProjekte/excel-liturgical-converter/catalogue.py",
    "references": "python C:/AppProjekte/excel-liturgical-converter/bible-references.py",
    "converter": "python C:/AppProjekte/excel-liturgical-converter/converter.py",
    "brevier": "python C:/AppProjekte/excel-liturgical-converter/converter.py -m 1",
    "deceased": "python C:/AppProjekte/excel-liturgical-converter/converter.py -m 2",
    "psalms": "python C:/AppProjekte/excel-liturgical-converter/converter.py -m 5",
    "lesungen1": "python C:/AppProjekte/excel-liturgical-converter/converter.py -m 11",
    "lesungen2": "python C:/AppProjekte/excel-liturgical-converter/converter.py -m 12",
    "upgrade": "node update-version.js",
    "update": "git add . && git commit && git push",
    "brevier-update": "python C:/AppProjekte/excel-liturgical-converter/converter.py -m 1 && git add . && git commit && git push",
    "predeploy": "set \"DISABLE_ESLINT_PLUGIN=true\" && git push && npm run build",
    "deploy": "gh-pages -d build && gh workflow run pages-build-deployment.yml",
    "start": "react-scripts start",
    "build": "set \"DISABLE_ESLINT_PLUGIN=true\" && react-scripts build",
    "test": "react-scripts test",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-syntax-flow": "^7.26.0",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@rushstack/eslint-patch": "^1.10.5",
    "autoprefixer": "^10.4.20",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.4",
    "gh-pages": "^6.2.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
